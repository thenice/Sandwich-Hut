<div id="pageTitle">
	<h1><%= @order.customer_id%></h1>
</div>

<div id="toolbar">
        <%= link_to "Back", :action => :index %>
        <%= link_to "Edit", :action => :edit, :id => @order.object %>
</div>


<div id="content">

    <table>
      
      <tr>
    	<td class="itemLabel">CustomerId</td>
        <td class="itemValue"><%= @order.customer_id%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">PickUpAt</td>
        <td class="itemValue"><%= @order.pick_up_at%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Total</td>
        <td class="itemValue"><%= @order.total%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">IsDelivery</td>
        <td class="itemValue"><%= @order.is_delivery%></td>
      </tr>
      
    </table>
</div>