<div class="pageTitle">
  <h1><%= @ordereditem.menu_item_id %></h1>
</div>

<div class="toolbar">
  <div class="leftItem backButton">
    <a href="<%= url_for :action => :index %>">Back</a>
  </div>
  <div class="rightItem regularButton">
    <a href="<%= url_for :action => :edit, :id => @ordereditem.object %>">Edit</a>
  </div>
</div>

<div class="content">
  <ul>
    
        <li>
          <div class="itemLabel">MenuItemId</div>
          <div class="itemValue"><%= @ordereditem.menu_item_id %></div>
        </li>
    
        <li>
          <div class="itemLabel">OrderId</div>
          <div class="itemValue"><%= @ordereditem.order_id %></div>
        </li>
    
        <li>
          <div class="itemLabel">IngredientsJson</div>
          <div class="itemValue"><%= @ordereditem.ingredients_json %></div>
        </li>
    
        <li>
          <div class="itemLabel">Total</div>
          <div class="itemValue"><%= @ordereditem.total %></div>
        </li>
    
        <li>
          <div class="itemLabel">SpecialInstructions</div>
          <div class="itemValue"><%= @ordereditem.special_instructions %></div>
        </li>
    
        <li>
          <div class="itemLabel">Status</div>
          <div class="itemValue"><%= @ordereditem.status %></div>
        </li>
    
  </ul>
</div>