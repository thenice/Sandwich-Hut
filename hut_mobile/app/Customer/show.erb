<div class="pageTitle">
  <h1><%= @customer.email_address %></h1>
</div>

<div class="toolbar">
  <div class="leftItem backButton">
    <a href="<%= url_for :action => :index %>">Back</a>
  </div>
  <div class="rightItem regularButton">
    <a href="<%= url_for :action => :edit, :id => @customer.object %>">Edit</a>
  </div>
</div>

<div class="content">
  <ul>
    
        <li>
          <div class="itemLabel">EmailAddress</div>
          <div class="itemValue"><%= @customer.email_address %></div>
        </li>
    
        <li>
          <div class="itemLabel">Password</div>
          <div class="itemValue"><%= @customer.password %></div>
        </li>
    
        <li>
          <div class="itemLabel">PaymentMethod</div>
          <div class="itemValue"><%= @customer.payment_method %></div>
        </li>
    
        <li>
          <div class="itemLabel">PaymentId</div>
          <div class="itemValue"><%= @customer.payment_id %></div>
        </li>
    
        <li>
          <div class="itemLabel">PaymentSecret</div>
          <div class="itemValue"><%= @customer.payment_secret %></div>
        </li>
    
        <li>
          <div class="itemLabel">Id</div>
          <div class="itemValue"><%= @customer.id %></div>
        </li>
    
  </ul>
</div>